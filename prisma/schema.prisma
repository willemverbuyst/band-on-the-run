generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Musician {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    name      String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Band {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    name      String   @unique
    bio       String?
    genre     String[]
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
